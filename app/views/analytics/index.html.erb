<%= form_tag(chart_path, method: :get) do %>
  <%= select_tag 'attribute', options_for_select(Mouse.attribute_names)%>
  <%= select_tag 'chart_type', options_for_select([["bar", "bar_chart"], ["line", "line_chart"], ["column", "column_chart"], ["pie", "pie_chart"], ["area", "area_chart"]])%>
  <%= submit_tag("Create Chart", class: 'card-button') %>
<% end %>
